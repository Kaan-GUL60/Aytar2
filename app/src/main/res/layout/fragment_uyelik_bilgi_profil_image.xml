<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/background_color"
        tools:context=".UyelikBilgiProfilImageFragment" >

    <ImageView
            android:src="@drawable/account_circle_48px"
            android:layout_width="127dp"
            android:layout_height="115dp" android:id="@+id/profilImageView"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/imagePicker" app:layout_constraintVertical_bias="0.317"
            tools:ignore="ContentDescription"/>
    <TextView
            android:text="@string/baslik40"

            android:layout_marginTop="15dp"
            android:textSize="20sp"
            android:textColor="@color/black"
            android:fontFamily="@font/poppins_bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView25"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/imagePicker"
            app:layout_constraintTop_toBottomOf="@+id/profilImageView"/>
    <Button
            android:text="@string/baslik41"
            android:layout_width="0dp"
            android:layout_margin="10dp"
            android:background="@drawable/green_button_bg"
            android:layout_height="wrap_content" android:id="@+id/imagePicker"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintTop_toBottomOf="@+id/profilImageView"/>
    <ScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imagePicker">
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                      android:orientation="vertical">
            <TextView
                    android:text="@string/baslik42"
                    android:layout_marginTop="15dp"
                    android:textSize="20sp"
                    android:textColor="@color/black"
                    android:fontFamily="@font/poppins_bold"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView34"/>
            <LinearLayout
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginTop="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                <ImageView
                        android:src="@drawable/account_circle_48px"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:id="@+id/hazirImage1" tools:ignore="ContentDescription"/>
                <ImageView
                        android:src="@drawable/account_circle_48px"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:id="@+id/hazirImage2" tools:ignore="ContentDescription"/>
                <ImageView
                        android:src="@drawable/account_circle_48px"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:id="@+id/hazirImage3" tools:ignore="ContentDescription"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginTop="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:layout_weight="1">
                <ImageView
                        android:src="@drawable/account_circle_48px"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:id="@+id/hazirImage4"
                        tools:ignore="ContentDescription,NestedWeights"/>
                <ImageView
                        android:src="@drawable/account_circle_48px"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:id="@+id/hazirImage5" tools:ignore="ContentDescription"/>
                <ImageView
                        android:src="@drawable/account_circle_48px"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:id="@+id/hazirImage6" tools:ignore="ContentDescription"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginTop="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:layout_weight="1">
                <ImageView
                        android:src="@drawable/account_circle_48px"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:id="@+id/hazirImage7" tools:ignore="ContentDescription,NestedWeights"/>
                <ImageView
                        android:src="@drawable/account_circle_48px"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:id="@+id/hazirImage8" tools:ignore="ContentDescription"/>
                <ImageView
                        android:src="@drawable/account_circle_48px"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:id="@+id/hazirImage9" tools:ignore="ContentDescription"/>
            </LinearLayout>
            <Button
                    android:text="@string/baslik43"
                    android:layout_margin="10dp"
                    android:background="@drawable/green_button_bg"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/button5"/>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>