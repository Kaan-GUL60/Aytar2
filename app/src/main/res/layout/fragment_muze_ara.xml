<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/background_color"
        tools:context=".MuzeAraFragment" >

    <LinearLayout
            android:orientation="vertical"
            android:gravity="center"
            android:background="@drawable/ust_bg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout2" tools:ignore="MissingConstraints">
        <LinearLayout
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <ImageView
                    android:src="@drawable/arrow_back_ios_48px"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/backImageMuze"
                    android:layout_weight="1"/>
            <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:hint="@string/muze_ara"
                    android:padding="10dp"
                    android:layout_weight="3"
                    android:layout_margin="30dp"
                    android:ems="10"
                    android:id="@+id/editTextTextPersonName"/>
        </LinearLayout>
    </LinearLayout>
    <ScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout2">
        <LinearLayout android:layout_width="match_parent"
                      android:gravity="center"
                      android:layout_height="wrap_content"
                      android:orientation="vertical">
            <LinearLayout
                    android:orientation="vertical"
                    android:gravity="center"
                    android:id="@+id/m1"
                    android:layout_margin="10dp"
                    android:background="@drawable/item_bg"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                <LinearLayout
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_margin="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    <TextView
                            android:text="@string/hint4"
                            android:layout_weight="4"

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/MuzeBaslik1"/>
                    <ImageView
                            android:src="@drawable/star"
                            android:layout_width="24dp"
                            android:gravity="center"
                            android:layout_height="24dp"
                            android:id="@+id/star1"
                            android:layout_weight="1"/>
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_margin="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    <TextView
                            android:text="@string/hint5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/muzeAciklama1"
                            android:layout_weight="2"/>
                    <ImageView
                            android:src="@drawable/ic_launcher_background"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/muzeResim1"
                            android:layout_weight="1"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                    android:orientation="vertical"
                    android:gravity="center"
                    android:id="@+id/m2"
                    android:layout_margin="10dp"
                    android:background="@drawable/item_bg"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                <LinearLayout
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_margin="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    <TextView
                            android:text="@string/hint4"
                            android:layout_weight="4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/MuzeBaslik2"/>
                    <ImageView
                            android:src="@drawable/star"
                            android:layout_width="24dp"
                            android:gravity="center"
                            android:layout_height="24dp"
                            android:id="@+id/star2"
                            android:layout_weight="1"/>
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_margin="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    <TextView
                            android:text="@string/hint5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/muzeAciklama2"
                            android:layout_weight="2"/>
                    <ImageView
                            android:src="@drawable/ic_launcher_background"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/muzeResim2"
                            android:layout_weight="1"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                    android:orientation="vertical"
                    android:gravity="center"
                    android:id="@+id/m3"
                    android:layout_margin="10dp"
                    android:background="@drawable/item_bg"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                <LinearLayout
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_margin="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    <TextView
                            android:text="@string/hint4"
                            android:layout_weight="4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/MuzeBaslik3"/>
                    <ImageView
                            android:src="@drawable/star"
                            android:layout_width="24dp"
                            android:gravity="center"
                            android:layout_height="24dp"
                            android:id="@+id/star3"
                            android:layout_weight="1"/>
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_margin="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    <TextView
                            android:text="@string/hint5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/muzeAciklama3"
                            android:layout_weight="2"/>
                    <ImageView
                            android:src="@drawable/ic_launcher_background"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/muzeResim3"
                            android:layout_weight="1"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                    android:orientation="vertical"
                    android:gravity="center"
                    android:id="@+id/m4"
                    android:layout_margin="10dp"
                    android:background="@drawable/item_bg"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                <LinearLayout
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_margin="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    <TextView
                            android:text="@string/hint4"
                            android:layout_weight="4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/MuzeBaslik4"/>
                    <ImageView
                            android:src="@drawable/star"
                            android:layout_width="24dp"
                            android:gravity="center"
                            android:layout_height="24dp"
                            android:id="@+id/star4"
                            android:layout_weight="1"/>
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_margin="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    <TextView
                            android:text="@string/hint5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/muzeAciklama4"
                            android:layout_weight="2"/>
                    <ImageView
                            android:src="@drawable/ic_launcher_background"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/muzeResim4"
                            android:layout_weight="1"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                    android:orientation="vertical"
                    android:gravity="center"
                    android:id="@+id/m5"
                    android:layout_margin="10dp"
                    android:background="@drawable/item_bg"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                <LinearLayout
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_margin="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    <TextView
                            android:text="@string/hint4"
                            android:layout_weight="4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/MuzeBaslik5"/>
                    <ImageView
                            android:src="@drawable/star"
                            android:layout_width="24dp"
                            android:gravity="center"
                            android:layout_height="24dp"
                            android:id="@+id/star5"
                            android:layout_weight="1"/>
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_margin="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    <TextView
                            android:text="@string/hint5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/muzeAciklama5"
                            android:layout_weight="2"/>
                    <ImageView
                            android:src="@drawable/ic_launcher_background"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/muzeResim5"
                            android:layout_weight="1"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>